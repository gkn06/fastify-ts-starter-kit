{
  "name": "fastify-ts-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "ts:lint": "tsc --noEmit --watch",
    "dev": "tsx watch src/index.ts",
    "dev:inspect": "tsx watch -- --inspect src/index.ts",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "dev:tsnd": "tsnd --respawn --transpile-only --exit-child src/index.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitest/coverage-c8": "^0.24.4",
    "@vitest/ui": "^0.24.4",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "lint-staged": "^13.0.3",
    "pino-pretty": "^9.1.1",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.11.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vitest": "^0.24.4",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "@fastify/autoload": "^5.4.1",
    "@fastify/cors": "^8.1.1",
    "@fastify/sensible": "^5.1.1",
    "@fastify/swagger": "^7.6.1",
    "fastify": "^4.9.2",
    "fastify-zod": "^1.2.0",
    "zod": "^3.19.1"
  }
}
